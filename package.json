{
  "name": "@asframe/opencode-iflow-auth",
  "version": "1.0.7",
  "description": "OpenCode plugin for iFlow.cn - Access Qwen, DeepSeek, Kimi, GLM-5 models with OAuth 2.0, API Key, and CLI Proxy support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write --no-config --no-semi --single-quote --trailing-comma none --print-width 100 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opencode",
    "plugin",
    "iflow",
    "glm-5",
    "qwen",
    "deepseek",
    "kimi",
    "ai",
    "auth",
    "oauth",
    "apikey",
    "cli-proxy"
  ],
  "author": "asframe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asframe/opencode-iflow-auth.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^1.2.6",
    "proper-lockfile": "^4.1.2",
    "tiktoken": "^1.0.17",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/proper-lockfile": "^4.1.4",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.7.3"
  },
  "opencode": {
    "type": "plugin",
    "hooks": [
      "auth",
      "event",
      "chat.headers"
    ]
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
